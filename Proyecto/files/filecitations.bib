  author={Diao, Boyu and Yang, Kang and Mei, Yuzhu and Wang, Qi and Li, Chao and An, Zhulin and Xu, Yongjun},
  booktitle={2019 IEEE International Conferences on Ubiquitous Computing & Communications (IUCC) and Data Science and Computational Intelligence (DSCI) and Smart Computing, Networking and Services (SmartCNS)}, 
  title={A Scalable Testbed for Task Offloading and Deployment of Heterogeneous Edge Computing}, 
  year={2019},
  volume={},
  number={},
  pages={586-591},
  abstract={Heterogeneous edge computing has become a new computing paradigm with the aim of moving the computation away from cloud towards the edge of the network. With the rapid increase in communication data volume, changing the network structure to provide hard real-time services is one of the goals of edge computing. A proper testbed is essential for fully validating edge computing algorithms, especially for computation offloading. In this paper, we present a scalable testbed for task offloading and deployment of heterogeneous edge computing adopting cross-platform container technique to encapsulate the software environment and runtime dependencies of heterogeneous computing units. extensive experiments have demonstrated that containers do not bring extra time consumption or occupy extra computing resources.},
  keywords={Cloud computing;Runtime;Scalability;Software algorithms;Containers;Software;Heterogeneous networks;Hardware;Resource management;Edge computing;Edge Computing, Testbed, Heterogeneous, Containers, Deployment},
  doi={10.1109/IUCC/DSCI/SmartCNS.2019.00123},
  ISSN={},
  month={Oct},}@INPROCEEDINGS{10541395,
  author={Anu and Solanki, Kamna and Dhankar, Amita},
  booktitle={2023 5th International Conference on Advances in Computing, Communication Control and Networking (ICAC3N)}, 
  title={A Survey on Energy efficiency in Cloud Computing Frameworks at Different Platforms}, 
  year={2023},
  volume={},
  number={},
  pages={1516-1521},
  abstract={Cloud computing is a paradigm that offers a restricted set of virtualized resources that may be accessed as needed. It is crucial to allocate these resources on target machines in a manner that minimizes both inadequate utilization and excess utilization, as these conditions can result in substantial consumption of energy. The term “nano data centers” refers to a network of widely dispersed computers that have the capability to both host and distribute material and solicitations in a peer-to-peer manner. The investigation of energy usage in nano servers has not been extensively explored, despite their increasing popularity. Several findings have been derived from our study, which highlight the aspects within the system design that enable nDCs to achieve lower energy consumption compared to their centralized counterparts. This paper presents a comprehensive definition of Cloud Frameworks, encompassing many platforms such as edge computing, green computing, parallel computing, distributed computing, and fog computing. In conclusion, this study highlights a number of obstacles and potential within the realm of cloud computing. It is our intention that this work will attract the interest of the academic community and serve as a catalyst for further investigation in this domain.},
  keywords={Surveys;Cloud computing;Data centers;Energy consumption;Reviews;Parallel processing;Software;Servers;System analysis and design;Edge computing;cloud computing;edge computing;nano servers;fog computing;parallel computing},
  doi={10.1109/ICAC3N60023.2023.10541395},
